Object subclass: #AlgoVid	instanceVariableNames: 'personas burbujas colegios'	classVariableNames: ''	package: 'TP1-106051'!!AlgoVid methodsFor: 'initialization' stamp: 'S 5/19/2021 11:54'!initialize 	personas := Dictionary new.	burbujas := Dictionary new.	colegios := Dictionary new.! !!AlgoVid methodsFor: 'as yet unclassified' stamp: 'SantiagoVaccarelli 5/15/2021 21:07'!estaVacunadx: unNombrePersona 	(self buscar: unNombrePersona en: personas) vacunar.		! !!AlgoVid methodsFor: 'as yet unclassified' stamp: 'SantiagoVaccarelli 5/18/2021 13:44'!puedeCircular: unNombrePersona	^((self buscar: unNombrePersona en: personas) devolverCirculacion).! !!AlgoVid methodsFor: 'as yet unclassified' stamp: 'S 5/19/2021 11:53'!nuevoColegio: unNombreColegio	|unColegio|	unColegio := Colegio  new.	unColegio asignarNombre: unNombreColegio.	self nombreDuplicado: unNombreColegio en: colegios.	colegios at: unNombreColegio put: unColegio.! !!AlgoVid methodsFor: 'as yet unclassified' stamp: 'SantiagoVaccarelli 5/15/2021 21:05'!clasesPresencialesEn: unNombreColegio 	^(self buscar: unNombreColegio en: colegios) clasesPresenciales.! !!AlgoVid methodsFor: 'as yet unclassified' stamp: 'S 5/19/2021 11:53'!nuevaBurbuja: unNombreBurbuja	|unaBurbuja|	unaBurbuja := Burbuja  new.	unaBurbuja asignarNombre: unNombreBurbuja.	self nombreDuplicado: unNombreBurbuja en: burbujas. 	burbujas at: unNombreBurbuja put: unaBurbuja.! !!AlgoVid methodsFor: 'as yet unclassified' stamp: 'S 5/19/2021 11:53'!buscar: unNombreObjeto en: unDiccionario	^(unDiccionario at: unNombreObjeto ifAbsent:[NombreInexistente signal]).! !!AlgoVid methodsFor: 'as yet unclassified' stamp: 'SantiagoVaccarelli 5/18/2021 13:54'!esPersonaDeRiesgo: nombrePersona 	(self buscar: nombrePersona en: personas) esDeRiesgo.! !!AlgoVid methodsFor: 'as yet unclassified' stamp: 'S 5/19/2021 11:54'!nombreDuplicado: unNombre en: unDiccionario	unDiccionario at: unNombre  ifPresent: [NombreDuplicado signal].! !!AlgoVid methodsFor: 'as yet unclassified' stamp: 'SantiagoVaccarelli 5/15/2021 21:04'!agregarA:unNombreBurbuja alColegio: unNombreColegio	(self buscar: unNombreColegio en: colegios) agregarA: (self buscar: unNombreBurbuja en: burbujas)! !!AlgoVid methodsFor: 'as yet unclassified' stamp: 'SantiagoVaccarelli 5/18/2021 14:22'!diagnosticoDe: unNombrePersona 	^(self buscar: unNombrePersona en: personas) devolverDiagnostico.! !!AlgoVid methodsFor: 'as yet unclassified' stamp: 'SantiagoVaccarelli 5/18/2021 13:53'!esPersonalEsencial: nombrePersona	(self buscar: nombrePersona en: personas) esEsencial .	! !!AlgoVid methodsFor: 'as yet unclassified' stamp: 'S 5/19/2021 11:53'!nuevaPersona: unNombrePersona	|unaPersona|	unaPersona := Persona new.	unaPersona asignarNombre: unNombrePersona.	self nombreDuplicado: unNombrePersona en: personas.	personas at: unNombrePersona put: unaPersona.! !!AlgoVid methodsFor: 'as yet unclassified' stamp: 'SantiagoVaccarelli 5/15/2021 21:04'!agregarA: unNombrePersona aLaBurbuja: unNombreBurbuja	(self buscar: unNombreBurbuja en: burbujas )agregarA: (self buscar: unNombrePersona en: personas) ! !!AlgoVid methodsFor: 'as yet unclassified' stamp: 'SantiagoVaccarelli 5/15/2021 23:23'!contactoEstrechoEntre: nombrePrimerPersona y: nombreSegundaPersona	(self buscar: nombrePrimerPersona en: personas) agregarContacto: (self buscar: nombreSegundaPersona en: personas).	(self buscar: nombreSegundaPersona en: personas) agregarContacto: (self buscar: nombrePrimerPersona en: personas).! !!AlgoVid methodsFor: 'as yet unclassified' stamp: 'S 5/20/2021 11:50'!burbujaPinchada: unNombreBurbuja	^(self buscar: unNombreBurbuja en: burbujas) estaPinchada.! !!AlgoVid methodsFor: 'as yet unclassified' stamp: 'SantiagoVaccarelli 5/15/2021 21:04'!agregarSintoma: unSintoma a: unNombrePersona  	(self buscar: unNombrePersona en: personas) agregarSintoma: unSintoma.	! !Object subclass: #Burbuja	instanceVariableNames: 'personas nombre estado'	classVariableNames: ''	package: 'TP1-106051'!!Burbuja methodsFor: 'as yet unclassified' stamp: 'SantiagoVaccarelli 5/12/2021 00:39'!asignarNombre: unNombreBurbuja 	nombre := unNombreBurbuja ! !!Burbuja methodsFor: 'as yet unclassified' stamp: 'S 5/20/2021 11:50'!estaPinchada	personas do:[:persona|persona chequearBurbuja: self].	^estado estaPinchada.! !!Burbuja methodsFor: 'as yet unclassified' stamp: 'S 5/20/2021 11:49'!agregarA: unaPersona 	personas add: unaPersona. 	unaPersona estaEnBurbuja: self.! !!Burbuja methodsFor: 'as yet unclassified' stamp: 'S 5/20/2021 11:50'!pinchar	estado := Pinchada new.	estado actualizarPersonasBurbuja: personas.! !!Burbuja methodsFor: 'initialization' stamp: 'S 5/20/2021 11:50'!initialize	personas := OrderedCollection new.	estado := NoPinchada new.! !!Burbuja methodsFor: 'accessing' stamp: 'SantiagoVaccarelli 5/18/2021 14:23'!devolverNombre	^nombre! !Object subclass: #BurbujaEstado	instanceVariableNames: ''	classVariableNames: ''	package: 'TP1-106051'!Object subclass: #Circulacion	instanceVariableNames: ''	classVariableNames: ''	package: 'TP1-106051'!Object subclass: #Clases	instanceVariableNames: ''	classVariableNames: ''	package: 'TP1-106051'!!Clases methodsFor: 'as yet unclassified' stamp: 'S 5/19/2021 14:44'!actualziarPresencialidad: colegio! !Object subclass: #Colegio	instanceVariableNames: 'nombre presencialidad burbujas'	classVariableNames: ''	package: 'TP1-106051'!!Colegio methodsFor: 'accessing' stamp: 'SantiagoVaccarelli 5/18/2021 14:23'!devolverNombre	^nombre.! !!Colegio methodsFor: 'as yet unclassified' stamp: 'SantiagoVaccarelli 5/12/2021 12:51'!asignarNombre: unNombreColegio	nombre := unNombreColegio ! !!Colegio methodsFor: 'as yet unclassified' stamp: 'S 5/19/2021 14:43'!agregarA: burbuja 	burbujas add: burbuja.! !!Colegio methodsFor: 'as yet unclassified' stamp: 'S 5/20/2021 13:59'!clasesPresenciales	(burbujas isEmpty) ifTrue: [ColegioVacio signal].	presencialidad actualziarPresencialidad: self.     ^presencialidad sonPresenciales.! !!Colegio methodsFor: 'as yet unclassified' stamp: 'S 5/19/2021 14:43'!devolverBurbujas	^burbujas! !!Colegio methodsFor: 'as yet unclassified' stamp: 'S 5/19/2021 14:47'!clasesNoPresenciales	presencialidad := NoPresenciales new.! !!Colegio methodsFor: 'initialization' stamp: 'S 5/19/2021 14:43'!initialize 	burbujas := OrderedCollection new.	presencialidad := Presenciales new.! !Object subclass: #Diagnostico	instanceVariableNames: ''	classVariableNames: ''	package: 'TP1-106051'!!Diagnostico methodsFor: 'as yet unclassified' stamp: 'Anonymous 5/19/2021 10:44'!actualizarBurbuja: unaBurbuja! !!Diagnostico methodsFor: 'as yet unclassified' stamp: 'S 5/19/2021 11:18'!agregarContacto: unaPersona! !!Diagnostico methodsFor: 'as yet unclassified' stamp: 'Anonymous 5/19/2021 10:44'!actualizarCirculacion: unaPersona ! !Circulacion subclass: #NoPermitida	instanceVariableNames: ''	classVariableNames: ''	package: 'TP1-106051'!!NoPermitida methodsFor: 'as yet unclassified' stamp: 'SantiagoVaccarelli 5/18/2021 14:21'!circulacionPermitida 	^false.! !BurbujaEstado subclass: #NoPinchada	instanceVariableNames: ''	classVariableNames: ''	package: 'TP1-106051'!!NoPinchada methodsFor: 'as yet unclassified' stamp: 'SantiagoVaccarelli 5/12/2021 10:01'!estaPinchada	^false.! !!NoPinchada methodsFor: 'as yet unclassified' stamp: 'SantiagoVaccarelli 5/15/2021 16:38'!actualizarPersonasBurbuja: personasBurbuja! !Clases subclass: #NoPresenciales	instanceVariableNames: ''	classVariableNames: ''	package: 'TP1-106051'!!NoPresenciales methodsFor: 'as yet unclassified' stamp: 'S 5/20/2021 13:59'!sonPresenciales	^false.! !Circulacion subclass: #Permitida	instanceVariableNames: ''	classVariableNames: ''	package: 'TP1-106051'!!Permitida methodsFor: 'as yet unclassified' stamp: 'SantiagoVaccarelli 5/18/2021 14:21'!circulacionPermitida 	^true.! !Object subclass: #Persona	instanceVariableNames: 'nombre circulacion riesgo burbuja diagnostico sintomas'	classVariableNames: ''	package: 'TP1-106051'!!Persona methodsFor: 'accessing' stamp: 'S 5/19/2021 11:33'!diagnostico 	^diagnostico.! !!Persona methodsFor: 'initialization' stamp: 'S 5/19/2021 14:30'!initialize 	circulacion := NoPermitida new.	diagnostico := Saludable new.	riesgo := PersonaNoRiesgo new.	burbuja := Burbuja new.	sintomas := OrderedCollection new.! !!Persona methodsFor: 'mensajesPersona' stamp: 'S 5/19/2021 14:25'!circulacionPermitida	circulacion := Permitida  new.	! !!Persona methodsFor: 'mensajesPersona' stamp: 'S 5/19/2021 11:16'!devolverDiagnostico	^diagnostico nombre.! !!Persona methodsFor: 'mensajesPersona' stamp: 'SantiagoVaccarelli 5/18/2021 14:23'!devolverNombre	^nombre.! !!Persona methodsFor: 'mensajesPersona' stamp: 'LuciaLiceri 5/10/2021 16:34'!asignarNombre: unNombrePersona  	nombre:= unNombrePersona.! !!Persona methodsFor: 'mensajesPersona' stamp: 'S 5/19/2021 14:25'!circulacionNoPermitida	circulacion := NoPermitida new.! !!Persona methodsFor: 'mensajesPersona' stamp: 'S 5/19/2021 12:06'!agregarSintoma: unSintoma	|sintomaNuevo|	sintomaNuevo := Sintoma new.	sintomaNuevo nombrar: unSintoma.	sintomas add: sintomaNuevo.	diagnostico actualizarDiagnostico: sintomas de: self. 	diagnostico actualizarCirculacion: self.! !!Persona methodsFor: 'mensajesPersona' stamp: 'SantiagoVaccarelli 5/17/2021 20:11'!estaEnBurbuja: unaBurbuja 	burbuja := unaBurbuja.! !!Persona methodsFor: 'mensajesPersona' stamp: 'S 5/19/2021 14:23'!diagnosticoSospechoso	diagnostico := Sospechoso new.	diagnostico actualizarBurbuja: burbuja.! !!Persona methodsFor: 'mensajesPersona' stamp: 'S 5/19/2021 11:02'!chequearBurbuja: unaBurbuja	diagnostico actualizarBurbuja: unaBurbuja.! !!Persona methodsFor: 'mensajesPersona' stamp: 'S 5/19/2021 11:03'!vacunar	diagnostico vacunar: self.! !!Persona methodsFor: 'mensajesPersona' stamp: 'S 5/19/2021 11:34'!agregarContacto: unaPersona	(unaPersona diagnostico) agregarContacto: self.	! !!Persona methodsFor: 'mensajesPersona' stamp: 'S 5/19/2021 14:30'!esDeRiesgo	riesgo := PersonaRiesgo new.	riesgo actualizarCirculacion: self.! !!Persona methodsFor: 'mensajesPersona' stamp: 'S 5/19/2021 14:30'!devolverRiesgo 	^riesgo.! !!Persona methodsFor: 'mensajesPersona' stamp: 'S 5/19/2021 14:30'!esEsencial	riesgo actualizarCirculacion: self.	diagnostico actualizarCirculacion: self.! !!Persona methodsFor: 'mensajesPersona' stamp: 'S 5/19/2021 14:23'!diagnosticoPositivo	diagnostico := Positivo new.	diagnostico actualizarBurbuja: burbuja.! !!Persona methodsFor: 'mensajesPersona' stamp: 'S 5/19/2021 12:01'!devolverCirculacion	^circulacion circulacionPermitida.! !BurbujaEstado subclass: #Pinchada	instanceVariableNames: ''	classVariableNames: ''	package: 'TP1-106051'!!Pinchada methodsFor: 'as yet unclassified' stamp: 'SantiagoVaccarelli 5/12/2021 09:58'!estaPinchada 	^true! !!Pinchada methodsFor: 'as yet unclassified' stamp: 'SantiagoVaccarelli 5/18/2021 14:21'!actualizarPersonasBurbuja: personasBurbuja	personasBurbuja do:[:persona| persona circulacionNoPermitida].! !Diagnostico subclass: #Positivo	instanceVariableNames: ''	classVariableNames: ''	package: 'TP1-106051'!!Positivo methodsFor: 'as yet unclassified' stamp: 'SantiagoVaccarelli 5/18/2021 14:21'!actualizarCirculacion: unaPersona 	unaPersona circulacionNoPermitida.! !!Positivo methodsFor: 'as yet unclassified' stamp: 'S 5/20/2021 11:50'!actualizarBurbuja: unaBurbuja	unaBurbuja pinchar.! !!Positivo methodsFor: 'as yet unclassified' stamp: 'S 5/19/2021 11:17'!nombre	^'Positivo'	! !!Positivo methodsFor: 'as yet unclassified' stamp: 'S 5/19/2021 11:19'!agregarContacto: unaPersona	unaPersona circulacionNoPermitida.! !Clases subclass: #Presenciales	instanceVariableNames: ''	classVariableNames: ''	package: 'TP1-106051'!!Presenciales methodsFor: 'as yet unclassified' stamp: 'S 5/20/2021 13:59'!sonPresenciales	^true.! !!Presenciales methodsFor: 'as yet unclassified' stamp: 'S 5/20/2021 11:50'!actualziarPresencialidad: colegio	|burbujas|	burbujas := colegio devolverBurbujas.	(((burbujas count: [:x|x estaPinchada ])/burbujas size) > 0.4) ifTrue: [colegio clasesNoPresenciales].! !Object subclass: #Riesgo	instanceVariableNames: ''	classVariableNames: ''	package: 'TP1-106051'!Riesgo subclass: #PersonaNoRiesgo	instanceVariableNames: ''	classVariableNames: ''	package: 'TP1-106051'!!PersonaNoRiesgo methodsFor: 'as yet unclassified' stamp: 'SantiagoVaccarelli 5/18/2021 14:21'!actualizarCirculacion: unaPersona	unaPersona circulacionPermitida.! !Riesgo subclass: #PersonaRiesgo	instanceVariableNames: ''	classVariableNames: ''	package: 'TP1-106051'!!PersonaRiesgo methodsFor: 'as yet unclassified' stamp: 'SantiagoVaccarelli 5/18/2021 14:21'!actualizarCirculacion: unaPersona	unaPersona circulacionNoPermitida.! !Diagnostico subclass: #Saludable	instanceVariableNames: ''	classVariableNames: ''	package: 'TP1-106051'!!Saludable methodsFor: 'accessing' stamp: 'S 5/19/2021 11:16'!nombre	^'Saludable'! !!Saludable methodsFor: 'as yet unclassified' stamp: 'S 5/19/2021 11:29'!actualizarDiagnostico: sintomas de: unaPersona	(sintomas size ~= 0) ifTrue: [unaPersona diagnosticoSospechoso].! !!Saludable methodsFor: 'as yet unclassified' stamp: 'SantiagoVaccarelli 5/18/2021 13:47'!vacunar: unaPersona 	(unaPersona devolverRiesgo) actualizarCirculacion: unaPersona.! !Object subclass: #Sintoma	instanceVariableNames: 'nombre habituales'	classVariableNames: ''	package: 'TP1-106051'!!Sintoma methodsFor: 'as yet unclassified' stamp: 'S 5/19/2021 12:21'!esHabitual	^(habituales includes: nombre). ! !!Sintoma methodsFor: 'as yet unclassified' stamp: 'S 5/20/2021 10:49'!nombrar: nombreNuevo	nombre := nombreNuevo.! !!Sintoma methodsFor: 'initialization' stamp: 'SantiagoVaccarelli 5/14/2021 12:39'!initialize 		habituales := OrderedCollection new.		habituales add: 'Fiebre'; add: 'Tos'; add: 'Cansancio'! !Diagnostico subclass: #Sospechoso	instanceVariableNames: ''	classVariableNames: ''	package: 'TP1-106051'!!Sospechoso methodsFor: 'accessing' stamp: 'S 5/19/2021 11:16'!nombre	^'Sospechoso'! !!Sospechoso methodsFor: 'as yet unclassified' stamp: 'S 5/19/2021 11:29'!actualizarDiagnostico: sintomas de: unaPersona	(sintomas size = 3) ifTrue: [sintomas detect: [ :x| (x esHabitual)] ifFound: [unaPersona diagnosticoPositivo]].	((sintomas size >= 4)) ifTrue: [unaPersona diagnosticoPositivo].! !!Sospechoso methodsFor: 'as yet unclassified' stamp: 'S 5/19/2021 11:18'!agregarContacto: unaPersona 	unaPersona circulacionNoPermitida.! !!Sospechoso methodsFor: 'as yet unclassified' stamp: 'SantiagoVaccarelli 5/18/2021 14:21'!actualizarCirculacion: unaPersona 	unaPersona circulacionNoPermitida.! !